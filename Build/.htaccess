<IfModule mod_headers.c>
    <FilesMatch "\.gz$">
        Header set Content-Encoding gzip
        Header set Cache-Control "public"
        Header unset Content-Type
        Header set Content-Type "application/javascript"
    </FilesMatch>
    <FilesMatch "\.br$">
        Header set Content-Encoding br
        Header set Cache-Control "public"
        Header unset Content-Type
        Header set Content-Type "application/javascript"
    </FilesMatch>
</IfModule>
